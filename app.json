{
  "expo": {
    "name": "Aura",
    "slug": "aura-app",
    "scheme": "aura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aurasustentable.aura",
      "userInterfaceStyle": "automatic",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aurasustentable.aura"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-black.png",
          "dark": {
            "image": "./assets/splash-white.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 700
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que $(PRODUCT_NAME) use tu ubicaci√≥n."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir que $(PRODUCT_NAME) acceda a tus fotos."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc168c1c-a77d-46c3-bde7-6b4e1c8c5697"
      }
    },
    "owner": "pollovaldes"
  }
}
